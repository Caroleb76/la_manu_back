model ExtraCost {
  id             String       @id @default(uuid())
  category       CategoryEnum
  val            String?

  interventionId String?
  Intervention   Intervention? @relation(fields: [interventionId], references: [id])

  files File[]
  @@map("ExtraCost") // Optional: match DB table name if needed
}

enum CategoryEnum {
  frais_de_repas
}
